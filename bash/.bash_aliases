alias x='exit'

## Colorize the ls output ##
alias ls='ls -FG'
 
## Use a long listing format ##
alias ll='ls -l'
 
## a quick way to get out of current directory ##
alias ..='cd ..'
alias ...='cd ../../'
alias .4='cd ../../../../'
alias .5='cd ../../../../..'

## GIT
alias c='clear'
alias gs='git status'
alias cs='c && gs'
alias gb='git branch'
alias mp='git checkout master && git pull'

## VLC
alias vlc='/Applications/VLC.app/Contents/MacOS/VLC'

# Compass Compile Compressed
alias ccc='compass compile --force -s compressed'
